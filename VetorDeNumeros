import java.util.Scanner;

public class VetorDeNumeros {
    public static void main(String[] args) {
        // Criando um vetor de tamanho 5 para armazenar os números
        int[] numeros = new int[5];
        
        // Criando um objeto Scanner para ler os dados do usuário
        Scanner scanner = new Scanner(System.in);
        
        // Lendo os 5 números e armazenando no vetor
        System.out.println("Digite 5 números inteiros:");

        for (int i = 0; i < 5; i++) {
            System.out.print("Número " + (i + 1) + ": ");
            numeros[i] = scanner.nextInt();
        }
        
        // Fechando o scanner
        scanner.close();
        
        // Exibindo os números armazenados no vetor
        System.out.println("\nOs números digitados são:");
        for (int i = 0; i < 5; i++) {
            System.out.println("Número " + (i + 1) + ": " + numeros[i]);
        }
    }
}
